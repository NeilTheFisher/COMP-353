    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "sql-template-tag": "^5.2.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
