{
  "name": "youth-soccer-club",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "HOSTNAME=0.0.0.0 node ./standalone/server.js",
